{
    "name": "javiereguiluz/easy-admin-demo",
    "license": "MIT",
    "type": "project",
    "description": "A Symfony demo application to showcase the EasyAdmin features.",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "DomBase\\DomAdminBundle\\": "base/admin-bundle/src/",
            "DomBase\\DomApiBundle\\": "base/api-bundle/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.1.3",
        "ext-http": "*",
        "ext-json": "*",
        "ext-pdo_sqlite": "*",
        "doctrine/common": "^2.8",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "^2.6,>=2.6.3",
        "doctrine/persistence": "^1.0",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "gedmo/doctrine-extensions": "^2.4",
        "jms/serializer-bundle": "^2.4",
        "jwage/easy-csv": "^0.0.6",
        "pagerfanta/pagerfanta": "~1.0,>=1.0.1|~2.0",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "^4.2",
        "symfony/cache": "^4.2",
        "symfony/cache-contracts": "^1.1",
        "symfony/config": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/doctrine-bridge": "^4.2",
        "symfony/dotenv": "*",
        "symfony/event-dispatcher": "^4.2",
        "symfony/expression-language": "^4.2",
        "symfony/finder": "^4.2",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/http-foundation": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/monolog-bundle": "*",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-apcu": "^1.6",
        "symfony/polyfill-mbstring": "^1.7",
        "symfony/property-access": "^4.2",
        "symfony/security-bundle": "^4.2",
        "symfony/security-csrf": "*",
        "symfony/serializer": "4.3.*",
        "symfony/templating": "*",
        "symfony/translation": "^4.2",
        "symfony/twig-bridge": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/validator": "^4.2",
        "symfony/yaml": "*",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.10",
        "vich/uploader-bundle": "1.8.6",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "symfony/browser-kit": "*",
        "symfony/debug-bundle": "*",
        "symfony/phpunit-bridge": "*",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "*",
        "symfony/web-server-bundle": "*",
        "doctrine/data-fixtures": "^1.3",
        "psr/log": "~1.0",
        "symfony/console": "^4.2",
        "symfony/css-selector": "^4.2",
        "symfony/dom-crawler": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "scripts": {
        "auto-scripts": {
            "ckeditor:install": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}
